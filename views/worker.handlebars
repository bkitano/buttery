        <!-- received orders list -->
        <h1 style='text-align: center'>{{college}} buttery</h1>
        <div class='container'>
            <h2>In Progress</h2>
            <table id='received-orders' class="table">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Order</th>
                </tr>
              </thead>
              <tbody id="received-orders-list">
                  {{#if received_orders}}
                      {{#each received_orders}}
                        <tr id={{this.id}}>
                            <td class='name'>{{this.name}}</td>
                            <td class='items'>{{this.order}}</td>
                            <td>
                                <button class="btn btn-success complete">Complete</button>
                            </td>
                        </tr>

                      {{/each}}
                  {{/if}}
              </tbody>
            </table>
        </div>
        
        <!-- completed orders list -->
        <div class='container'>
            <h2>Orders Ready for Pick Up</h2>
            <table id='completed-orders' class="table">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Order</th>
                </tr>
              </thead>
              <tbody id="completed-orders-list">
                  {{#if completed_orders}}
                      {{#each completed_orders}}
                        <tr id={{this.id}}>
                            <td class='name'>{{this.name}}</td>
                            <td class='items'>{{this.order}}</td>
                            <td>
                                <button class="btn btn-primary picked-up">Picked Up</button>
                            </td>
                        </tr>
                      {{/each}}
                  {{/if}}
              </tbody>
            </table>
        </div>
        
        <!-- order form -->
        <div class='container'>
            <h2>Submit an Order</h2>
            <form class='form'>
                <div class='form-group' id="order-form">
                        <label for="name">Your Name</label>
                        <input class='form-control' type="text" id="name">
                        </br>
                        <label for="name">Order</label>
                        <input class='form-control' type="text" id="order">
                        </br>
                        <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.1/socket.io.js"></script>
        <script src='assets/js/worker_commands.js'></script>